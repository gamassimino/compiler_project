class person{
  integer age;
  integer day;
  integer month;
  integer year;

  void printInt(integer a) extern;

  void setAge(integer an_age){
    age = an_age;
    return;
  }

  void setDay(integer a_day){
    day = a_day;
    return;
  }

  void setMonth(integer a_month){
    month = a_month;
    return;
  }

  void setYear(integer a_year){
    year = a_year;
    return;
  }

  void printDay(){
    printInt(day);
    return;
  }

  void printMonth(){
    printInt(month);
    return;
  }

  void printYear(){
    printInt(year);
    return;
  }

  void printAge(){
    printInt(age);
    return;
  }
  void printDob(){
    printInt(day);
    printInt(month);
    printInt(year);
    return;
  }

}

