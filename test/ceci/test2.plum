class main{

  integer breaks(integer g){
    integer  i;
    i = 0;

    while(i < 10){
      i = i + 1;
      if(g < 0){
        break;
      }else{
        continue;
       }
     } //fin while
    return i;
  }

  void printI(integer i)extern;

  integer main(){
    printI(breaks(1));
    return 1;
  }

}
